{
  "name": "landing-web",
  "version": "1.22.26",
  "private": true,
  "scripts": {
    "start": "umi dev --port=8200",
    "start:fast": "cross-env NODE_OPTIONS=--max_old_space_size=4096 FAST=startFast umi dev --port=8200",
    "start:fastMock": "cross-env NODE_OPTIONS=--max_old_space_size=4096 FAST=startFast USEMOCK=1 umi dev --port=8200",
    "dev:mock": "USEMOCK=1 umi dev --port=8200",
    "dev:host": "HOST=land.kucoin.net PORT=80 umi dev",
    "dev:gbiz:local": "GBIZ=local umi dev --port=3000",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=6144 umi build && node ./scripts/version.js && yarn run build:offline",
    "build:analyze": "ANALYZE=1 umi build && node ./scripts/version.js",
    "build:offline": "./node_modules/.bin/mk-offline",
    "build:manifest": "node ./scripts/manifest.js",
    "test": "kc-web-test",
    "test:kc": "umi test",
    "proxy": "node ./scripts/proxy.js",
    "check:git:add": "./node_modules/.bin/checkGitAdd",
    "check:locale": "node ./scripts/checkLocale.js",
    "check:keys": "node ./scripts/checkNoUseKey.js",
    "lint": "eslint --ext .js src --quiet",
    "precommit": "lint-staged",
    "neeko:init": "neeko init",
    "neeko:m": "neeko import -m",
    "neeko:r": "neeko import -r",
    "neeko:exportALL": "neeko export -a",
    "neeko:exportDiff": "neeko export -i",
    "ci": "insight i ci",
    "prepare": "husky install",
    "check:depAnalyzer": "kc-web-checker -t depAnalyzer",
    "pre-commit": "insight inspect dev --verbose --except=units",
    "check:e2e": "kc-web-checker -t checkE2E",
    "test:dev": "insight inspect dev -v --item=units"
  },
