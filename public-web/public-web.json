{
  "url": "ssh://git@bitbucket.kucoin.net/kuc/public-web.git",
  "app_type": "web",
  "web_type": "static",
  "pre_copy": false,
  "node_version": "v14.19.3",
  "docker_image": "harbor-hkidc.kcprd.com/base/node:14.19.3",
  "offline_params": {
    "build_cmd": "yarn run build:sit",
    "build_offline": "yarn run build:offline",
    "dist_copy": {
      "on_opts": true,
      "copy_file": "dist",
      "copy_path": "public-web",
      "copy_cmd": "rsync",
      "exclude_file": ["public-web"],
      "delete_opts": true
    },
    "cdn_copy": {
      "on_opts": true,
      "copy_file": "dist/public-web/{ver}",
      "copy_path": "public-web/{ver}",
      "copy_cmd": "rsync",
      "exclude_file": [],
      "delete_opts": false
    },
    "other_opts": ""
  }
}
