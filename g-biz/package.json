{
  "name": "root",
  "version": "0.13.4",
  "private": true,
  "scripts": {
    "cz": "git-cz",
    "build:externals": "NODE_ENV=production rollup -c && node scripts/copy-import-map.js",
    "watch:externals": "NODE_ENV=development SITE_ENV=dev NODE_OPTIONS=--max_old_space_size=2048 rollup -c -w",
    "build:sit": "NODE_ENV=production rollup -c && node scripts/copy-import-map.js && cross-env yarn run build:offline",
    "build:dev": "NODE_ENV=production SITE_ENV=dev rollup -c && node scripts/copy-import-map.js && cross-env yarn run build:offline",
    "build:offline": "./node_modules/.bin/mk-offline",
    "start": "serve lib --cors -p 5001",
    "dev:externals": "rm -rf ./lib/* && NODE_ENV=development npm-run-all --parallel watch:externals start",
    "dev:fast-externals": "rm -rf ./lib/* && NODE_ENV=development DEV_FAST=1 npm-run-all --parallel watch:externals start",
    "watch": "lerna run --parallel watch",
    "watch2": "lerna run watch --scope @kc/entrance",
    "dev": "lerna exec --scope gbiz-dev -- parcel index.html --port 8000",
    "clean": "rimraf packages/*/es packages/*/lib",
    "lint": "eslint --ext .js packages externals",
    "lint:fix": "eslint --ext .js packages externals --fix",
    "commitlint": "commitlint -e $HUSKY_GIT_PARAMS",
    "prepare": "husky install",
    "pre-commit": "insight inspect dev -v",
    "postinstall": "patch-package",
    "ci": "insight i ci",
    "test": "jest --clearCache && jest",
    "test:dev": "insight inspect dev -v --item=units",
    "i18n": "node ./scripts/i18n-tool.js",
    "i18n:all": "node ./scripts/i18n-tool.js project=all"
  },
  "workspaces": [
    "packages/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "(externals|packages)/**/!(*test).js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.24.0",
    "@babel/plugin-external-helpers": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/plugin-transform-arrow-functions": "^7.10.1",
    "@babel/plugin-transform-regenerator": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.24.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/css": "^11.7.1",
    "@emotion/react": "11.4.1",
    "@kc/mui": "0.7.8-beta.10",
    "@kufox/icons": "1.19.25",
    "@kufox/makestyle": "1.1.4",
    "@kux/icons": "1.1.15",
    "@kux/mui": "1.2.19",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-url": "^4.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "14.4.3",
    "antd": "4.15.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.6",
    "babel-plugin-lodash": "^3.3.4",
    "classnames": "^2.3.2",
    "commitizen": "^4.0.3",
    "cross-env": "^7.0.3",
    "cz-lerna-changelog": "^2.0.2",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "eslint-plugin-ui-module": "^1.0.9",
    "husky": "^6.0.0",
    "inquirer": "^7.0.6",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "^26.6.2",
    "lerna": "^3.20.2",
    "lint-staged": "^10.5.4",
    "lodash-es": "4.17.21",
    "md5": "^2.3.0",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "offline-sources": "1.0.19",
    "patch-package": "^8.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "react-fast-marquee": "^1.6.4",
    "react-intl-universal": "1.3.4",
    "react-slick": "^0.28.1",
    "react-virtualized": "9.22.3",
    "rollup": "^2.36.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-import-css": "^3.0.2",
    "rollup-plugin-mv": "^0.0.2",
    "rollup-plugin-output-manifest": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "semver": "^7.5.4",
    "serve": "^12.0.1",
    "slick-carousel": "^1.8.1",
    "stylis": "^4.3.0",
    "stylis-plugin-rtl": "^2.1.1",
    "urlparser": "0.3.9",
    "vault-webpack-plugin": "^0.0.6"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.1",
    "@emotion/styled": "^11.11.0",
    "@hcaptcha/react-hcaptcha": "^1.4.4",
    "@kc/gbiz-base": "^1.2.2",
    "@knb/native-bridge": "1.0.7",
    "@kufox/mui": "2.6.0",
    "@kux/iconpack": "1.0.1-beta.20250930071927",
    "@loadable/component": "^5.15.3",
    "@optimize-lodash/rollup-plugin": "^4.0.3",
    "@sumsub/websdk": "2.3.1",
    "bignumber.js": "^9.1.0",
    "core-js": "^3.33.2",
    "dayjs": "^1.11.5",
    "decimal.js": "^10.4.1",
    "dom-to-image": "^2.6.0",
    "eslint-plugin-es-x": "^7.2.0",
    "html-to-react": "^1.5.0",
    "html2canvas": "^1.4.1",
    "i18next-http-backend": "^2.4.2",
    "path-to-regexp": "2.2.1",
    "prop-types": "^15.8.1",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-helmet-async": "^1.3.0",
    "react-lazy-load-image-component": "^1.6.0",
    "rollup-plugin-visualizer": "^5.9.0"
  },
  "packageManager": "yarn@1.22.22",
  "volta": {
    "node": "20.17.0",
    "yarn": "1.22.22"
  }
}
