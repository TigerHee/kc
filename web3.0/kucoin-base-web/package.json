{
  "name": "kucoin-base-web",
  "version": "0.9.2",
  "private": true,
  "scripts": {
    "build": "cross-env REACT_APP_CDN=https://assets.staticimg.com craco build",
    "build:a": "cross-env ANALYZE=1 REACT_APP_CDN=https://assets.staticimg.com craco build",
    "build:offline": "./node_modules/.bin/mk-offline",
    "build:sit": "cross-env REACT_APP_CDN=https://assets.staticimg.com craco build",
    "ci": "insight i ci",
    "lint": "eslint src --ext .js --quiet",
    "pre-commit": "insight inspect dev --verbose --except=units",
    "prepare": "husky install",
    "start": "cross-env REACT_APP_CDN=https://assets.staticimg.com craco start",
    "start:au": "yarn start site=au",
    "start:cl": "yarn start site=cl",
    "start:demo": "yarn start site=demo",
    "start:eu": "yarn start site=eu",
    "start:th": "yarn start site=th",
    "start:tr": "yarn start site=tr",
    "test": "kc-web-test",
    "test:dev": "insight inspect dev -v --item=units"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@kc/bridge": "^1.0.2",
    "@kc/performance-metric": "0.0.5",
    "@kc/sensors": "3.1.5",
    "@kc/web-kunlun": "0.0.3",
    "@kufox/icons": "^1.19.25",
    "@kufox/mui": "2.6.0",
    "@kux/icons": "1.1.15",
    "@kux/mui": "1.2.14",
    "@loadable/component": "^5.15.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "dva-core": "^2.0.4",
    "dva-loading": "^3.0.22",
    "history-with-query": "^4.10.4",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "store2": "^2.14.2",
    "stylis": "^4.1.3",
    "stylis-plugin-rtl": "^2.1.1",
    "systemjs-webpack-interop": "^2.3.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@kc/mk-plugin-mock": "^1.0.0-beta.20250326062456",
    "@umijs/fabric": "^3.0.0",
    "@umijs/test": "^3.4.11",
    "babel-plugin-lodash": "^3.3.4",
    "craco-interpolate-html-plugin": "^0.0.6",
    "cross-env": "^7.0.3",
    "crypto": "1.0.1",
    "customize-cra": "^1.0.0",
    "husky": "^8.0.3",
    "kc-web-eslint": "^0.6.1-beta.1",
    "kc-web-test": "^0.6.2",
    "offline-sources": "1.0.19",
    "prettier": "^2.8.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "volta": {
    "node": "14.19.3"
  }
}
