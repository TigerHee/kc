    "start:gbiz:local:fast": "cross-env PORT=3000 FAST=startFast GBIZ=local umi dev",
    "start:mfsu": "cross-env PORT=3000 MFSU=mfsu umi dev",
    "build": "cross-env APP_CDN=https://assets.staticimg.com natasha build && node scripts/postBuild.js",
    "build:cn": "cross-env APP_CDN=https://assets.staticimg.com natasha build && node scripts/postBuild.js",
