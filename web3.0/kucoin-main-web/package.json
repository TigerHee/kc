    "build:offline": "./node_modules/.bin/mk-offline",
    "prettier": "prettier --write 'src/**/*.js'",
    "lint": "eslint src --ext .js --quiet",
    "lint:fix": "eslint src --ext .js --fix",
    "test": "kc-web-test",
    "jest": "jest --coverage",
    "inspect:test": "inspect cdnAssets/inspect-case.js -h",
    "ci": "insight i ci",
    "prepare": "husky install",
    "postinstall": "patch-package",
    "compress:lottie": "cross-env NODE_ENV=dev node scripts/compress-lottie.js",
    "pre-commit": "insight inspect dev --verbose --except=units",
    "test:dev": "insight inspect dev -v --item=units"
  },
