{
  "name": "trade-web",
  "version": "4.4.39",
  "private": true,
  "scripts": {
    "analyzer-useless": "cross-env USELESS=true yarn build",
    "build": "natasha build && node ./scripts/version.js && node ./scripts/manifest.js",
    "build:analyzer": "cross-env ANALYZER=true yarn build",
    "check:commitlint": "kc-web-checker -t commitlint",
    "check:detect": "kc-web-checker -t detect",
    "check:git:add": "node scripts/checkGitAdd.js",
    "check:i18n": "node ./scripts/unicode_check.js",
    "check:locale": "node scripts/checkLocale.js",
    "check:owner": "kc-web-checker -t owner --diff",
    "check:size": "kc-web-checker -t size --path src public --exclude locale BotLocale lightweight-charts.development.mjs --limit 150",
    "ci": "kc-web-checker -t owner,kuxold,test,eslint,ts",
    "clr443": "kill -9 `lsof -i tcp:8000 | grep node | awk '{print  $2}'`",
    "count-uncovered": "cross-env PASS_PCT=60 node ./scripts/countUncoveredTestFiles.js",
    "lint": "./node_modules/.bin/eslint --ext .js src",
    "lint:fix": "./node_modules/.bin/eslint --ext .js src --fix",
    "lint:quiet": "./node_modules/.bin/eslint --ext .js src --quiet",
    "lint:test": "./node_modules/.bin/eslint",
    "pre-commit": "kc-web-checker -t owner,eslint,kuxold,size,checkE2E --diff --path src cdnAssets --exclude lottie locales",
    "pre-commit:local": "kc-web-checker -t owner,kuxold,size,checkE2E --diff --path src cdnAssets --exclude lottie locales",
    "prepare": "husky install",
    "release": "node ./scripts/release.js",
    "start": "natasha start --port 8100",
    "start:clean": "rm -rf ./node_modules/.cache && yarn start",
    "svgr": "svgr -d src/trade4.0/components/Icons/ src/trade4.0/assets/icons",
    "test": "kc-web-test",
    "test:coverage": "kc-web-test --coverage",
    "test:get:uncovered": "yarn test:coverage && yarn count-uncovered",
    "check:e2e": "kc-web-checker -t checkE2E"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "git add"
    ]
  },
