{
  "parser": "babel-eslint",
  "plugins": ["react-hooks"],
  "extends": ["airbnb"],
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "_PUBLIC_PATH_": false,
    "_NATASHA_PATH_": false,
    "_PRD_": false,
    "_DEV_": false,
    "_IS_TEST_ENV_": false,
    "_APP_NAME_": false,
    "_VERSION_": false,
    "_SITE_": false,
    "_RELEASE_": false,
    "_API_HOST_": false,
    "_GATE_WAY_": false,
    "_KC_LOCALE_DATA": false,
    "_KC_PAGE_LANG_LOADER": false,
    "_RUNTIME_CONFIG_": false,
    "_IS_TEST_": false,
    "_IS_INSIDE_WEB_": false,
    "_XVERSION_": false,
    "_DESC_": false,
    "_ENV_": false,
    "SENTRY_DEBUG": false,
    "LOCAL_SENTRY_DEBUG": false,
    "_IS_SANDBOX_": false,
    "Systemjs": false,
    "Datafeeds": false,
    "TradingView": false,
    "System": false,
    "__webpack_public_path__": false,
    "CHARTING_PATH": false,
    "cy": false,
    "Cypress": false,
    "expect": false
  },
  "rules": {
    "generator-star-spacing": [0],
    "camelcase": [0],
    "consistent-return": [0],
    "react/forbid-prop-types": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "global-require": [0],
    "import/prefer-default-export": [0],
    "react/jsx-no-bind": [0],
    "react/no-multi-comp": [0],
    "react/no-unused-prop-types": [0],
    "react/prop-types": [0],
    "react/prefer-stateless-function": [0],
    "react/sort-comp": [0],
    "no-underscore-dangle": [0],
    "no-else-return": [0],
    "no-restricted-syntax": [0],
    "import/first": [2, "DISABLE-absolute-first"],
    "import/no-extraneous-dependencies": [0],
    "no-unused-vars": [0],
    "no-use-before-define": [0],
    "jsx-a11y/anchor-has-content": [0],
    "jsx-a11y/img-has-alt": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "no-nested-ternary": [0],
    "arrow-body-style": [0],
    "import/extensions": [0],
    "max-len": [
      2,
      {
        "code": 100,
        "tabWidth": 2,
        "ignorePattern": "^\\s*[<svg|<path].+>",
        "ignoreUrls": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true,
        "ignoreComments": true
      }
    ],
    "no-bitwise": [0],
    "no-cond-assign": [0],
    "import/no-unresolved": [0],
    "require-yield": [0],
    "padded-blocks": [0],
    "arrow-parens": [0],
    "no-constant-condition": [0],
    "no-console": [0],
    "no-script-url": [0],
    "no-param-reassign": [0],
    "no-restricted-properties": [0],
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
    "template-curly-spacing": "off",
    "indent": "off",
    "react/jsx-indent": "off",
    "no-mixed-operators": [0],
    "quotes": "off",
    // "react-hooks/rules-of-hooks": "warn",
    // "react-hooks/exhaustive-deps": "warn",
    "no-unused-expressions": ["warn", { "allowShortCircuit": true }],
    "no-undef": "error"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "legacyDecorators": true,
      "experimentalObjectRestSpread": true
    }
  }
}
