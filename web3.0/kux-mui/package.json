{
  "name": "root",
  "version": "1.0.0-beta.0",
  "private": true,
  "scripts": {
    "cz": "git-cz",
    "build": "lerna run --parallel build",
    "build:umd": "lerna run --parallel build --scope @kux/icons && lerna run --parallel build:umd --scope @kux/mui",
    "watch": "lerna run --parallel watch",
    "dev": "lerna exec --scope docs -- start-storybook -p 7001",
    "clean": "rimraf packages/*/es packages/*/lib",
    "lint": "lint-staged",
    "commitlint": "commitlint -e $HUSKY_GIT_PARAMS",
    "version": "rimraf .changeset && node scripts/version.js",
    "release": "node scripts/release.js",
    "test": "jest",
    "prepare": "husky install",
    "check:commitlint": "kc-web-checker -t commitlint"
  },
  "workspaces": [
    "packages/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  },
  "lint-staged": {
    "packages/*/src/*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-arrow-functions": "^7.10.1",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/plugin-transform-regenerator": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@emotion/babel-plugin": "^11.9.2",
    "@emotion/cache": "11.4.0",
    "@emotion/jest": "^11.7.1",
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-url": "^4.0.0",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/dom": "7.31.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.13",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-app": "^10.0.0",
    "clsx": "^1.1.1",
    "commitizen": "^4.0.3",
    "console.table": "^0.10.0",
    "cz-lerna-changelog": "^2.0.2",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^5.1.1",
    "glob": "^7.1.6",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^7.0.6",
    "jest": "^26.6.3",
    "jest-axe": "^5.0.1",
    "jest-cli": "^26.6.3",
    "kc-web-checker": "0.5.4-beta.e8cab7d",
    "lerna": "^3.20.2",
    "lint-staged": "^10.5.4",
    "lodash-es": "^4.17.21",
    "moment": "2.30.1",
    "ora": "5.4.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react-app-polyfill": "^2.0.0",
    "react-syntax-highlighter": "^15.4.3",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
