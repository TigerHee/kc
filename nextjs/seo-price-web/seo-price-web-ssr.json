{
  "url": "ssh://git@bitbucket.kucoin.net/kuc/seo-price-web.git",
  "app_type": "web",
  "web_type": "static",
  "pre_copy": true,
  "node_version": "v22.14.0",
  "docker_image": "harbor-hkidc.kcprd.com/base/node:22.14.0-bullseye",
  "offline_params": {
    "build_cmd": "yarn run build:all",
    "dist_copy": {
      "on_opts": true,
      "copy_file": "dist",
      "copy_path": "seo-price-web-ssr",
      "copy_cmd": "rsync",
      "exclude_file": ["seo-price-web", "server-functions"],
      "delete_opts": true
    },
    "cdn_copy": {
      "on_opts": true,
      "copy_file": "dist/seo-price-web/{ver}",
      "copy_path": "seo-price-web/{ver}",
      "copy_cmd": "rsync",
      "exclude_file": [],
      "delete_opts": false
    },
    "other_opts": ""
  }
}
