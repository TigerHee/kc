  "main": "index.js",
  "exports": {
    "./*": "./dist/*.js"
  },
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:ssr && npm run build:systemjs",
    "build:ssr": "rollup -c",
    "build:systemjs": "NODE_ENV=production rollup -c rollup-gbiz.config.js",
    "build:offline": "./node_modules/.bin/mk-offline",
    "dev": "cross-env NODE_ENV=development node scripts/watchDev.js",
    "push:project": "cross-env NODE_ENV=development yarn build:ssr && yarn copy:project",
    "copy:project": "rm -rf ../brisk-web-ssr/node_modules/gbiz-next && cp -r dist ../brisk-web-ssr/node_modules/gbiz-next",
    "build:analyze": "ANALYZE=true rollup -c",
    "eslint": "eslint --quiet ./src",
    "version": "node scripts/version.js",
    "release": "node scripts/publish.js",
    "lint": "lint-staged",
    "ci": "insight i ci",
    "start:server": "serve dist/assets --cors -p 5002",
    "watch:externals": "IS_PROXY_DEV=true NODE_ENV=development SITE_ENV=dev NODE_OPTIONS=--max_old_space_size=4096 rollup -w -c rollup-gbiz.config.js",
    "dev:externals": "rm -rf dist && IS_PROXY_DEV=true NODE_ENV=development npm-run-all --parallel watch:externals start:server",
    "dev:ssr": "rm -rf dist && NODE_ENV=development npm-run-all --parallel dev start:server",
    "test:dev": "insight inspect dev -v --item=units"
  },
  "lint-staged": {
    "(externals|packages)/**/!(*test).js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "files": [
    "dist",
    "README.md"
  ],
