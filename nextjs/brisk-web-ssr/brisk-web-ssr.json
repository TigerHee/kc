{
  "url": "ssh://git@bitbucket.kucoin.net/kuc/brisk-web-ssr.git",
  "app_type": "web",
  "web_type": "static",
  "pre_copy": true,
  "node_version": "v22.14.0",
  "docker_image": "harbor-hkidc.kcprd.com/base/node:22.14.0-bullseye",
  "offline_params": {
    "build_cmd": "yarn run build:all",
    "dist_copy": {
      "on_opts": true,
      "copy_file": "dist",
      "copy_path": "brisk-web-ssr",
      "copy_cmd": "rsync",
      "exclude_file": [
        "brisk-web-ssr",
        "server-functions"
      ],
      "delete_opts": true
    },
    "cdn_copy": {
      "on_opts": true,
      "copy_file": "dist/brisk-web-ssr/{ver}",
      "copy_path": "brisk-web-ssr/{ver}",
      "copy_cmd": "rsync",
      "exclude_file": [],
      "delete_opts": false
    },
    "other_opts": ""
  }
}
