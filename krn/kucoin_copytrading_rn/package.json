{
  "name": "kucoin_copytrading_rn",
  "version": "1.0.25",
  "private": true,
  "scripts": {
    "build": "yarn build-ios && yarn build-android",
    "build-android": "yarn run init && cross-env PLATFORM=android node src/scripts/buildBundle",
    "build-ios": "yarn run init && cross-env PLATFORM=ios node src/scripts/buildBundle",
    "build:sit": "BUILD_ENV=SIT yarn build-ios && yarn build-android",
    "ci": "insight i ci",
    "dev": "node --max_old_space_size=16000 node_modules/react-native/cli.js start  --port=8082",
    "init": "node src/scripts/autoRegisterModels.js",
    "lint": "eslint ./src --ext .js",
    "pre-commit": "insight inspect dev --verbose --except=units",
    "prepare": "husky install",
    "start": "yarn run init && react-native start --port=8082",
    "test:watch": "cross-env BABEL_ENV=test jest --watch",
    "test": "cross-env BABEL_ENV=test jest --all",
    "test:dev": "insight inspect dev -v --item=units",
    "test-web-test": "kc-web-test",
    "unused-check": "yarn run init && cross-env PLATFORM=ios UNUSED_CHECK=1 node src/scripts/buildBundle",
    "unused-check:delete": "yarn run init && cross-env PLATFORM=ios UNUSED_CHECK=2 node src/scripts/buildBundle"
  },
