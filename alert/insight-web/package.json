{
  "name": "insight-web",
  "version": "0.1.10",
  "private": true,
  "description": "前端开发者平台",
  "author": "lucas.zhou@kupotech.com",
  "repository": "",
  "scripts": {
    "dev": "npm run start:dev",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "jest": "jest",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "prepare": "husky",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "npm run build && max preview --port 8000",
    "test": "jest",
    "test:coverage": "npm run jest -- --coverage",
    "test:update": "npm run jest -- -u",
    "tsc": "tsc --noEmit",
    "deploy": "docker-compose build",
    "ci": "insight i ci",
    "pre-commit": "insight inspect dev --verbose --except=units",
    "test:dev": "insight inspect dev -v --item=units"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.8.3",
    "@ant-design/pro-components": "^2.8.1",
    "ahooks": "^3.8.1",
    "antd": "^5.22.0",
    "antd-style": "^3.7.0",
    "boring-avatars": "^1.11.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "highlight.js": "^11.10.0",
    "lottie-web": "^5.12.2",
    "pnpm": "^9.12.3",
    "querystring": "^0.2.1",
    "random-words": "^2.0.1",
    "rc-resize-observer": "^1.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-sticky-box": "^2.0.5",
    "socket.io-client": "^4.8.1",
    "three": "^0.171.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^3.3.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.21",
    "@types/history": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.10",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/three": "^0.171.0",
    "@umijs/lint": "^4.3.24",
    "@umijs/max": "^4.3.24",
    "@umijs/plugins": "^4.3.31",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "express": "^4.21.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "mockjs": "^1.1.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "umi-presets-pro": "^2.0.3",
    "umi-serve": "^1.9.11"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
