{
  "name": "insight-server",
  "version": "0.1.13",
  "description": "前端开发者平台",
  "author": "lucas.zhou@kupotech.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy": "docker-compose build",
    "deploy:dev": "docker-compose -f docker-compose.dev.yml build",
    "ci": "insight i ci",
    "pre-commit": "insight inspect dev --verbose",
    "test:dev": "insight inspect dev -v --item=units",
    "prepare": "husky"
  },
  "dependencies": {
    "@azure/msal-node": "^2.15.0",
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.3",
    "@hokify/agenda": "^6.3.0",
    "@nestjs/axios": "^3.1.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/platform-socket.io": "^10.4.7",
    "@nestjs/websockets": "^10.4.7",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "conventional-commits-parser": "5.0.0",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "he": "^1.2.0",
    "https-proxy-agent": "^7.0.5",
    "joplin-turndown-plugin-gfm": "^1.0.12",
    "jsdom": "^25.0.1",
    "jwks-rsa": "^3.1.0",
    "lottie-compress": "^1.1.4",
    "mongoose": "^8.7.3",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "puppeteer": "^22.10.0",
    "random-words-commonjs": "^2.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "source-map": "^0.7.4",
    "turndown": "^7.2.0",
    "ulid": "^2.3.0",
    "vault-webpack-plugin": "^0.0.6",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/conventional-commits-parser": "^5.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}