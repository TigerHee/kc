{
  "name": "sentry-notice",
  "version": "1.1.15",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node index.js --port 2999 --debug true",
    "check:commitlint": "kc-web-checker -t commitlint",
