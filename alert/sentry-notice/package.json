    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.5",
    "dotenv": "^8.2.0",
    "es6-promise": "^4.2.8",
    "express": "^4.21.1",
    "hpagent": "^1.2.0",
    "lodash": "^4.17.21",
    "log4js": "6.4.0",
    "node-schedule": "^2.1.0"
  },
  "devDependencies": {
    "kc-web-checker": "^0.5.9"
  }
}
